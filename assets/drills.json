---
layout: null
permalink: /assets/drills.json
---
[
{% assign items = site.drills | sort: "title" %}
{% for d in items %}
  {
    "title": {{ d.title | jsonify }},
    "url": {{ d.url | relative_url | jsonify }},
    "short_description": {{ d.short_description | default: "" | strip_newlines | jsonify }},
    "thumbnail": {{ d.thumbnail | default: "" | relative_url | jsonify }},
    "categories": {{ d.categories | jsonify }},
    "equipment": {{ d.equipment | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
